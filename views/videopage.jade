extend layout

block content
  script(src='/js/vidpage.js')
  p
    | This is the individual player 

  #videocontain
    span.LimelightEmbeddedPlayer
      script(href='//video.limelight.com/player/embed.js')
      object(type='application/x-shockwave-flash', id='limelight_player_clip', name='limelight_player_clip', class='LimelightEmbeddedPlayerFlash', 
      width='330', height='186', data='//video.limelight.com/player/loader.swf')
